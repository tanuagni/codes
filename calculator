
int main()
{
int a=4,b=5,r;
char ch;
scanf("%c",&ch);
switch(ch)
{
	case '+':
		r=a+b;
		printf("%d",r);		
		break;
	case '-':
	  r=a-b;
	  printf("%d",r);	
	  break;
	case '*':
	r=a*b;
	printf("%d",r);	
	break;
	case '/':
	r=a/b;
	printf("%d",r);	
	break;
	default:
	printf("error");	  
	  
	  	
}

}
